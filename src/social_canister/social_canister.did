type CreatingGroup = record {
  governorate_id : nat8;
  city_id : nat16;
  name : text;
  parent_group_id : opt text;
  sport_type : Sports;
  description : text;
  public : bool;
  image : opt blob;
};
type Group = record {
  id : text;
  members : nat;
  governorate_id : nat8;
  city_id : nat16;
  name : text;
  parent_group_id : opt text;
  sport_type : Sports;
  description : text;
  public : bool;
  created_at : nat64;
  created_by : principal;
  image : blob;
  posts : nat;
};
type GroupFilter = record {
  governorate_id : opt nat8;
  city_id : opt nat16;
  sport_type : opt Sports;
  group_type : opt GroupType;
};
type GroupMember = record {
  user_id : principal;
  group_id : text;
  joined_at : nat64;
};
type GroupType = variant { Club; Group };
type Like = record {
  liked_at : nat64;
  user_id : principal;
  target : LikeTarget;
};
type LikeTarget = variant { Post : text; Comment : text };
type NewPost = record {
  content : text;
  group_id : text;
  images : opt vec blob;
};
type Post = record {
  updated_at : nat64;
  post_id : text;
  content : text;
  created_at : nat64;
  author : principal;
  likes : nat64;
  group_id : text;
  comments : nat64;
  images : opt vec blob;
};
type Result = variant { Ok : Group; Err : text };
type Result_1 = variant { Ok : Post; Err : text };
type Result_2 = variant { Ok; Err : text };
type Sports = variant {
  Basketball;
  Tennis;
  Volleyball;
  Football;
  Padel;
  Fitness;
  Cycling;
  Handball;
  Camping;
  Running;
  Skateboarding;
  Swimming;
};
type UpdatePost = record {
  post_id : text;
  content : opt text;
  images : opt vec blob;
};
service : {
  create_group : (CreatingGroup) -> (Result);
  create_post : (NewPost) -> (Result_1);
  delete_group : (text) -> (Result_2);
  delete_post : (text) -> (Result_2);
  filter_groups : (GroupFilter) -> (vec Group) query;
  get_current_user_posts : () -> (vec Post) query;
  get_group : (text) -> (Result) query;
  get_group_members : (text) -> (vec GroupMember) query;
  get_group_posts : (text) -> (vec Post) query;
  get_member_groups : (principal) -> (vec Group) query;
  get_post : (text) -> (Result_1) query;
  get_post_likes : (text) -> (vec Like) query;
  get_posts_by_user : (principal) -> (vec Post) query;
  get_sub_club : (text, text) -> (vec Group) query;
  get_sub_clubs : (text) -> (vec Group) query;
  get_unseen_posts : () -> (vec Post) query;
  get_user_likes : () -> (vec Like) query;
  join_group : (text) -> (Result_2);
  leave_group : (text) -> (Result_2);
  like : (LikeTarget) -> (Result_2);
  mark_post_as_read : (text) -> (Result_2);
  unlike : (LikeTarget) -> (Result_2);
  update_post : (text, UpdatePost) -> (Result_1);
}
